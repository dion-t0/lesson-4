import random

# Generate 20 random marks for students
randommarks=[random.randint(0,100) for i in range(20)]


print("20 random marks for 20 students :" , (randommarks))
list_a=[]
list_b=[]
list_c=[]

for i in randommarks:
    if i  <=30:
        list_a.append(i)
    elif i >30 and i <=70:
        list_b.append(i)
    elif i >70 and i <=100:
        list_c.append(i)




print(list_a)
print(list_b)
print(list_c)


